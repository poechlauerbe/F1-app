{
  "name": "my-f1-app",
  "version": "0.3.0",
  "description": "My small F1 app. Used API: [OpenF1](https://openf1.org/)",
  "main": "server.js",
  "scripts": {
    "start": "mkdir -p log && node ./server 2>&1 | tee -a ./log/stderr_$(date +'%Y-%m-%d-%H').log",
    "dev": "mkdir -p log && nodemon server.js 2>&1 | tee -a ./log/stderr_$(date +'%Y-%m-%d-%H').log"
  },
  "keywords": [
    "F1",
    "formula1",
    "App",
    "express",
    "node.js",
    "ejs"
  ],
  "author": "poechlauerbe",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.10",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.19.2",
    "ical": "^0.8.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^4.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.5.1",
    "jsonc-eslint-parser": "^2.4.0",
    "nodemon": "^3.1.4"
  }
}
